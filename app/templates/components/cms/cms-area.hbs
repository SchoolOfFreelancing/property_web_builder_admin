{{!-- based on 3 col section on langing page --}}
<div class="" style="padding-bottom: 3%;min-height: 160px;">
  <h3 class="text_center m-b-20">{{t cmsPartInfo.tabTitleKey }}  
    <span class="smaller-font">({{t labelSuffixKey }})</span> 
    {{#unless isEditing}}
      <span class="m-l-10 smaller-font"  {{action "startEditing" }}><a href=""><i class="fa fa-pencil"></i> {{t "edit"}}</a></span>
    {{/unless}}
  </h3>

  {{#if isEditing}}
    <div class="comfy-edit-container">
    {{cms/cms-part blocksInfo=cmsPartInfo.editorBlocks blocksContent=cmsPage
    updateCachesAction="updateCaches"  isEditing=isEditing}}
    </div>
  {{else}}

    <div class="comfy-display-container">
    {{{ cmsPage.contentCache }}}
    </div>

  {{/if}}

  <div>
