{{#if isActive}}
<div role="tabpanel" class="tab-pane" id="descripcion">
  <div class="row">
    <div class="col-sm-12">
      <div>
        {{#if hasChanged}} {{#if hasErrors}} {{!--
        <div class="pull-left">{{t "prompts.fixErrors" }}</div> --}} {{else}}
        <button type="submit" class="pull-left btn btn-primary btn-sm m-b-10 waves-effect" {{action "saveAgencyDetails"}}>
          {{t "save"}}
        </button>
        {{/if}} {{/if}}
      </div>
      <div>
        {{#upload/x-droplet url="/api/v1/properties/1/photo"}} {{/upload/x-droplet}}
      </div>
    </div>
  </div>
    <div class="row">
      {{#each propertyPhotos as |photo|}}
      <div class="col-sm-4">
        <h2>..</h2>
        <img src={{ photo.image.url }}>
      </div>
      {{/each}}
    </div>
</div>
{{/if}}
