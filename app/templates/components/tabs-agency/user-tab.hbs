{{#if isActive}}
<div role="tabpanel" class="tab-pane active" id="general">
  <div class="row">
    {{partial "partials/manage-changes-buttons" }}
    <div class="col-sm-6">


      {{#each leftInputFields as |inputField|}}
      <div class="form-group fg-float">
          {{partial "partials/input-field-resolver" }}
      </div>
      {{/each}}
      <br>
    </div>
    <div class="col-sm-6">


      {{#each rightInputFields as |inputField|}}
      <div class="form-group fg-float">
          {{partial "partials/input-field-resolver" }}
      </div>
      {{/each}}
      <br>
    </div>
  </div>
</div>
{{/if}}
