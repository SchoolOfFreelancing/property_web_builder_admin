{{#if fieldDetails.tooltipTextTKey}}
  <a class="ayuda" data-toggle="tooltip" title="" data-original-title={{t fieldDetails.tooltipTextTKey}}><i class="fa fa-info"></i></a>
{{/if}}

  <label class="fg-label ">{{t fieldDetails.labelTextTKey }}</label>
<div class="fg-line has-">

  <select class="selectpicker" style="display: none;">
{{#each fieldOptions as |selectValue|}} 
  <option value={{ selectValue.value }} >{{ selectValue.label }}</option>
{{/each}}
  </select>

{{#each fieldOptions as |selectValue|}} 
  <div class="radio m-b-15">
    <label {{action "radioChanged" selectValue this}}>
      <input type="radio"  
       value={{ selectValue.value }} checked={{ selectValue.checked}}>
      <i class="input-helper"></i> {{ selectValue.label }}
    </label>
  </div>
{{/each}}



</div>
