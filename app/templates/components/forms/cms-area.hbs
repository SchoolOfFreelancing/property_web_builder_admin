{{!-- based on 3 col section on langing page --}}
<div class="col-sm-12" style="padding-bottom: 3%;min-height: 160px;">
  <h4>{{t cmsPages.label }}  <span class="in-lang">({{t labelSuffixKey }})</span></h4>

  {{#if isEditing}}
    <div>
    {{cms/cms-part blocksInfo=cmsPartInfo.editorBlocks blocksContent=cmsPages
    updateCachesAction="updateCaches"  isEditing=isEditing}}
    </div>
  {{else}}

    <div>
    {{{ cmsPages.contentCache }}}
    </div>

  {{/if}}

  <div>
    {{#unless isEditing}}
      <p class="m-t-10"  {{action "startEditing" }}><a href=""><i class="fa fa-pencil"></i> {{t "edit"}}</a></p>
    {{/unless}}
  </div>


