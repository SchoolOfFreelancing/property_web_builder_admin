{{#if fieldDetails.tooltipText}}
  <a class="ayuda" data-toggle="tooltip" title="" data-original-title={{fieldDetails.tooltipText}}><i class="fa fa-info"></i></a>
{{/if}}
<div class="fg-line has-">
  <select class="selectpicker" style="display: none;">
{{#each fieldDetails.options as |selectValue|}} 
  <option>{{ selectValue.titleKey }}</option>
{{/each}}
  </select>

</div>
<label class="fg-label arriba_label">{{t fieldDetails.labelText }}</label>

