<div role="tabpanel" class="tab-pane">
  <div class="row">
    {{partial "partials/manage-changes-buttons" }}

    <div class="col-sm-6">
      <h4>Logo</h4> {{#upload/x-droplet url=addPhotoEndpoint addPhotosFromUrlsAction="addPhotosFromUrls" didUploadAction="refreshPhotos"}} {{/upload/x-droplet}}
      <table id="tabla_fotos" cellspacing="0">
        {{#each landingPagePhotos as |photo| }}
        <tr data-id_foto="1" id="1" class="foto_principal">
          <td>
            <div class="thumbnail">
              <img src={{ photo.image.url }} alt="">
            </div>
            <div class="hidden-xs">{{ photo.image.url }}</div>
          </td>
          <td class="text-left">
            <button type="button" class="btn btn-icon command-delete" {{action "deletePhoto" photo }}>
              <span class="md md-delete"></span>
            </button>
          </td>
        </tr>
        {{/each}}
      </table>
      <p><a href="#"><i class="fa fa-plus"></i> AÃ±adir foto</a></p>
    </div>
    <div class="col-sm-6">
      {{#each socialNetworkFields as |fieldDetails|}} 

        {{forms/social-input resetTrigger=resetTrigger valueChangedAction="checkDirtyState" fieldDetails=fieldDetails resourceObject=resourceObject}}
      {{/each}}
    </div>
  </div>
</div>
