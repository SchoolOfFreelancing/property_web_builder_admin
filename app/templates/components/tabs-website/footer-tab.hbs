<div role="tabpanel" class="tab-pane">
  <div class="row">
    {{partial "partials/manage-changes-buttons" }}
    <div class="col-sm-6">
      <h4>{{t "inTabLabels.socialLinks"}}</h4> 
      {{#each socialNetworkFields as |fieldDetails|}} {{forms/social-input resetTrigger=resetTrigger valueChangedAction="checkDirtyState" fieldDetails=fieldDetails resourceObject=resourceObject}} 
      {{/each}}
    </div>
    <div class="col-sm-6">
      {{#each contentResources as |contentItem|}}
      {{!-- <h3 class="text-center" style="">{{t contentItem.labelKey }} </h3>  --}}
        {{#each languages as |languageSettings|}} {{forms/content-item languageSettings=languageSettings contentItem=contentItem saveContentItemAction="saveContentItem"}} 
        {{/each}} 
      {{/each}}
    </div>
    {{partial "partials/manage-changes-buttons" }}
  </div>
</div>
