<div role="tabpanel" class="tab-pane">
  <div class="row">
    <div class="col-sm-12">
      <div class="alert alert-info alert-dismissible" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Cerrar"><span aria-hidden="true"></span></button><i class="fa fa-lg fa-info"></i>
          {{t cmsPartInfo.tabInfoKey }}
      </div>
    </div>
  </div>
  <div class="row">
    {{#each contentResources as |cmsPages| }}
    <div class="col-sm-12">
      <div class="" style="">
        {{cms/cms-area cmsPages=cmsPages 
        cmsPartInfo=cmsPartInfo updateCachesAction="updateCaches"
        }}
      </div>
    </div>
    {{/each}}
  </div>
  <div class="row">
  </div>
</div>
