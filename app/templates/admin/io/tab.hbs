<div role="tabpanel">
  <ul data-tab-color="green" class="tab-nav" role="tablist" tabindex="1" style="display:none;overflow: hidden; outline: none;">
    {{!--
    --}} 
    {{#each tabsList as |tab|}}
    <li class={{if (eq tab.tabValue activeTabName) "active"}}>
      {{#link-to 'admin.io.tab' tab.tabValue}}{{t tab.tabTitleKey}}{{/link-to}}</li>
    {{/each}}
  </ul>
  <div class="tab-content">
    {{component tabs-io-component activeTabName=activeTabName activeTabObject=activeTabObject }}

  </div>
</div>
