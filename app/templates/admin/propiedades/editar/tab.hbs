<div role="tabpanel">
  <ul data-tab-color="green" class="tab-nav" role="tablist" tabindex="1" style="overflow: hidden; outline: none;">
{{!--     <li class="active"><a href="#general" aria-controls="general" role="tab" data-toggle="tab">General</a></li>
 --}}

    {{#each tabsList as |tab|}}
        <li class={{if (eq tab.tabValue activeTabName) "active"}} >{{#link-to 'admin.propiedades.editar.tab' tab.tabValue}}{{tab.tabTitle}}{{/link-to}}</li>  
    {{/each}}

  </ul>
  <div class="tab-content">


    {{tabs/general-tab activeTabName=activeTabName propertyResource=property fieldKeys=fieldKeys }}

    {{tabs/description-tab activeTabName=activeTabName propertyResource=property }}

    {{tabs/situacion-tab activeTabName=activeTabName propertyResource=property fieldKeys=fieldKeys }}

    {{tabs/extras-tab activeTabName=activeTabName propertyResource=property }}

    {{tabs/venta-tab activeTabName=activeTabName propertyResource=property }}


  </div>

</div>


