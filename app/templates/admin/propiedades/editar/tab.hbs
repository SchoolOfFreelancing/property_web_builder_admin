<div role="tabpanel">
  <ul data-tab-color="green" class="tab-nav" role="tablist" tabindex="1" style="overflow: hidden; outline: none;">
{{!--     <li class="active"><a href="#general" aria-controls="general" role="tab" data-toggle="tab">General</a></li>
 --}}

    {{#each tabsList as |tab|}}
        <li class={{if (eq tab.tabValue activeTabName) "active"}} >{{#link-to 'admin.propiedades.editar.tab' tab.tabValue}}{{t tab.tabTitleKey}}{{/link-to}}</li>  
    {{/each}}

  </ul>
  <div class="tab-content">
{{!-- might use dynamic component thing like I do with admin/content/tab.hbs --}}
{{!-- referring to property as resourceObject is usefull as it allows me to use generic ref in base/tab-with-form --}}
    {{tabs-property/general-tab activeTabName=activeTabName resourceObject=property fieldKeys=fieldKeys }}

    {{tabs-property/description-tab activeTabName=activeTabName propertyResource=property }}

    {{tabs-shared/location-tab activeTabName=activeTabName resourceObject=property fieldKeys=fieldKeys }}

    {{tabs-property/extras-tab activeTabName=activeTabName propertyResource=property  fieldKeys=fieldKeys }}

    {{tabs-property/venta-tab activeTabName=activeTabName resourceObject=property }}

    {{tabs-property/owner-tab activeTabName=activeTabName resourceObject=property fieldKeys=fieldKeys}}

    {{tabs-property/fotos-tab activeTabName=activeTabName resourceObject=property }}


  </div>

</div>


